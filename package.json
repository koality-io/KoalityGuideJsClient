{
  "name": "@webpros/koality-guide-client",
  "version": "1.1.3",
  "description": "The KoalityGuide client connects a JavaScript application to the guides.",
  "author": "Nils Langner (nils.langner@leankoala.com)",
  "license": "MIT",
  "main": "./dist/GuideClient.js",
  "module": "./dist/GuideClient.esm.js",
  "typings": "./dist/types/GuideClient.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/koality-io/KoalityGuideJsClient.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "generateTypes": "tsc --emitDeclarationOnly --declaration --project tsconfig.types.json",
    "generateBuild": "node esbuild.js",
    "prePublish": "npm run generateBuild && npm run generateTypes",
    "clean": "rimraf coverage dist",
    "lint": "eslint --format=pretty --cache --max-warnings 0 --fix --ext .ts .",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "tslib": "2.4.0",
    "esbuild": "^0.14.38",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-plugin-jest": "26.1.5",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "16.x"
  }
}
